{
  "name": "cloudflare-ai-image-generator",
  "version": "1.0.0",
  "description": "AI Image Generator using Cloudflare Workers AI FLUX.1 [schnell]",
  "main": "src/index.js",
  "scripts": {
    "dev": "wrangler dev --env development",
    "dev:remote": "wrangler dev --env development --remote",
    "build": "npm run lint && npm run type-check",
    "lint": "eslint src/ --ext .js,.ts",
    "lint:fix": "eslint src/ --ext .js,.ts --fix",
    "lint:check": "eslint src/ --ext .js,.ts",
    "type-check": "tsc --noEmit",
    "test": "npm run verify-config && npm run lint:check",
    "verify-config": "node scripts/verify-config.js",
    "environment-check": "node scripts/environment-check.js",
    "setup": "chmod +x *.sh && ./init.sh",
    "setup:kv": "node scripts/setup-kv.js",
    "setup:env": "node scripts/setup-env.js",
    "deploy": "npm run pre-deploy && wrangler deploy",
    "deploy:dev": "./deploy.sh --env development",
    "deploy:staging": "./deploy.sh --env staging",
    "deploy:production": "./deploy.sh --env production",
    "deploy:quick": "./quick-deploy.sh",
    "pre-deploy": "npm run verify-config && npm run lint:check",
    "post-deploy": "node scripts/post-deploy.js",
    "logs": "wrangler tail",
    "logs:staging": "wrangler tail --env staging",
    "kv:create": "wrangler kv:namespace create IMAGE_STORE",
    "kv:create:preview": "wrangler kv:namespace create IMAGE_STORE --preview",
    "kv:list": "wrangler kv:key list --binding IMAGE_STORE",
    "kv:stats": "node scripts/cleanup-kv.js stats",
    "kv:cleanup": "node scripts/cleanup-kv.js cleanup",
    "kv:clear": "node scripts/cleanup-kv.js clear",
    "clean": "rm -rf dist/ .wrangler/ node_modules/.cache/",
    "fresh-install": "npm run clean && npm install",
    "init": "./init.sh",
    "health-check": "node scripts/post-deploy.js"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20240529.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.9.2",
    "wrangler": "^4.34.0"
  },
  "keywords": [
    "cloudflare-workers",
    "ai",
    "image-generation",
    "flux",
    "text-to-image"
  ],
  "author": "Your Name",
  "license": "MIT"
}
